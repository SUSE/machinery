  <general>
    <ask-list config:type="list">
      <ask>
        <question>Enter URL to system description</question>
        <default>http://</default>
        <file>/tmp/description_url</file>
        <stage>initial</stage>
        <script>
          <rerun_on_error config:type="boolean">true</rerun_on_error>
          <environment config:type="boolean">true</environment>
          <source><![CDATA[curl -f --head $VAL/manifest.json && exit 0 || exit 1]]></source>
        </script>
      </ask>
    </ask-list>
  </general>
  <scripts>
    <chroot-scripts config:type="list">
      <script>
        <source><![CDATA[mkdir -p '/mnt/etc'
curl -o '/mnt/etc/crontab' "`cat /tmp/description_url`/config_files/etc/crontab"
chown root:root '/mnt/etc/crontab'
chmod 644 '/mnt/etc/crontab'
mkdir -p '/mnt/usr/share/bash/helpfiles'
curl -o '/mnt/usr/share/bash/helpfiles/read' "`cat /tmp/description_url`/changed_managed_files/usr/share/bash/helpfiles/read"
chown root:root '/mnt/usr/share/bash/helpfiles/read'
chmod 644 '/mnt/usr/share/bash/helpfiles/read'
      curl -o '/mnt/tmp/filter' "`cat /tmp/description_url`/unmanaged_files_build_excludes"

        curl -o '/mnt/tmp/files.tgz' "`cat /tmp/description_url`/unmanaged_files/files.tgz"
        tar -C /mnt/ -X '/mnt/tmp/filter' -xf '/mnt/tmp/files.tgz'
        rm '/mnt/tmp/files.tgz'

        curl -o '/mnt/tmp/unmanaged-tree.tgz' "`cat /tmp/description_url`/unmanaged_files/trees/etc/unmanaged-tree.tgz"
        tar -C /mnt/ -X '/mnt/tmp/filter' -xf '/mnt/tmp/unmanaged-tree.tgz'
        rm '/mnt/tmp/unmanaged-tree.tgz'
]]></source>
      </script>
    </chroot-scripts>
  </scripts>
